{
  "name": "@nerimity/flakeid",
  "version": "1.0.3",
  "description": "Time based 64-bit unique id generator, inspired by Twitter id (snowflake)",
  "main": "dist/flakeid.js",
  "module": "dist/flakeid.mjs",
  "scripts": {
    "build": "rollup -c && pnpm create-dec",
    "create-dec": "tsc --allowJs -d --emitDeclarationOnly dist/flakeid.js dist/flakeid.mjs",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nerimity/FlakeId.git"
  },
  "keywords": [
    "snowflake",
    "flake",
    "unique",
    "id",
    "generator",
    "64-bit",
    "id",
    "generator"
  ],
  "author": "Supertiger",
  "license": "MIT",
  "homepage": "https://github.com/Nerimity/FlakeId#readme",
  "devDependencies": {
    "rollup": "^4.1.4",
    "typescript": "^5.2.2"
  },
  "bugs": {
    "url": "https://github.com/Nerimity/FlakeId/issues"
  }
}
